<Files database.php>
order allow,deny
Deny from all
</Files>

RewriteEngine on
RewriteCond %{HTTP_HOST} ^octamotor\.confusa\.top$ [OR]
RewriteCond %{HTTP_HOST} ^www\.octamotor\.confusa\.top$
RewriteCond %{REQUEST_URI} !^/[0-9]+\..+\.cpaneldcv$
RewriteCond %{REQUEST_URI} !^/\.well-known/pki-validation/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^/?$ "https\:\/\/confusa\.top\/octamotor" [R=301,L]


  #RewriteCond %{HTTP_HOST} ^(.+)\.confusa\.top$
  #RewriteRule ^(.*)$ https://%1.confusa.top$1 [R=301,L]
  
  
#RewriteCond %{SERVER_PORT} 80 
#RewriteRule ^(.*)$ https://confusa.top/$1 [R,L]



RewriteCond %{HTTP:X-Forwarded-Proto} !https
RewriteCond %{HTTPS} off
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]


RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ https://%1/$1 [R=301,L]

Options -Indexes

php_value session.gc_maxlifetime 7200


